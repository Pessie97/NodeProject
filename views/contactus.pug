extends default
block content

	form(action="/submitContact" method="POST" name="contact")
		p Please enter your information and someone will get back to you shortly.
		p First Name
			input(type="text" name="fname" value="")
			br
		p Last Name
			input(type = "text" name="lname" value="")
			br
		p Email Address
			input(type="email" name="email" value="")
			br
		p Message
			textarea(type="text" name="message" rows="7" cols="50")
			br
			
		input(type="submit" name="submit" value="Submit")
			